{
  "name": "jupyterlab_cjson",
  "version": "0.20.2",
  "description": "A JupyterLab extension for rendering CJSON",
  "author": "Chris Harris <chris.harris@kitware.com>",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "**/*.css"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/**/*.css lib",
    "watch": "tsc -w",
    "rm": "rimraf lib"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^0.4.0",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/widgets": "^1.3.0",
    "lodash": "^4.17.4",
    "mongochemclient": "git+https://github.com/OpenChemistry/mongochemclient.git#iso",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "universal-cookie": "^2.0.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.76",
    "@types/material-ui": "^0.18.1",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^5.0.9",
    "@types/react-tap-event-plugin": "0.0.30",
    "copyfiles": "^1.2.0",
    "rimraf": "^2.5.2",
    "typescript": "~2.3.1"
  }
}
